        <dependency>
            <groupId>org.springframework.security.oauth</groupId>
            <artifactId>spring-security-oauth2</artifactId>
            <version>2.0.3.RELEASE</version>
        </dependency>

        <!-- java image -->
        <dependency>
            <groupId>com.twelvemonkeys.imageio</groupId>
